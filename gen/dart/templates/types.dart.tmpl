{{define "types"}}
//
// Types
//

{{- if .Messages -}}
{{range .Messages -}}




{{if .Type | isEnum -}}
{{$enumName := .Name}}
enum {{$enumName}} {
{{- range $i, $field := .Fields}}
  {{- if $i}},{{end}}
  {{$field.Name}}
{{- end}}
}
{{end -}}

{{- if .Type | isStruct  }}
class {{.Name}} {
{{- range .Fields}}
  final {{.Type | fieldType}}{{" "}}{{if . | exportableField -}}{{. | exportedJSONField}};{{- end -}}{{end}}

  {{.Name}}({
  {{- range .Fields}}
  {{- if not .Optional}}
    @required this.{{ if . | exportableField -}}{{. | exportedJSONField}};{{- end -}}
  {{else}}
    this.{{ if . | exportableField -}}{{. | exportedJSONField}};{{- end -}}
  {{- end -}}
  {{- end}}
  });

}
{{- end -}}


{{end -}}
{{end -}}
{{end -}}
